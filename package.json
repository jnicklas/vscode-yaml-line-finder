{
  "name": "vscode-yaml-line-finder",
  "displayName": "YAML Lne Finder",
  "description": "Find YAML keys and jump to their line numbers. Useful for working with translation files and configuration files.",
  "version": "1.0.0",
  "repository": "https://github.com/jnicklas/vscode-yaml-line-finder",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yamlLineFinder.findKeyInCurrentFile",
        "title": "YAML Line Finder: find in current file"
      },
      {
        "command": "yamlLineFinder.jumpToTranslationFile",
        "title": "YAML Line Finder: jump to translation file"
      }
    ],
    "configuration": [
      {
        "title": "YAML Line Finder",
        "properties": {
          "yamlLineFinder.translationFiles": {
            "type": "array",
            "default": [],
            "description": "An array of translation files. Example: [{ \"languages\": [\"erb\"], \"path\": \"config/locales/en.yml\", \"keyPrefix\": \"en\" }]",
            "scope": "resource"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "yaml": "^2.3.0"
  }
}
